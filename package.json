{
  "name": "@aeternity/aepp-components",
  "version": "1.0.7",
  "description": "Reusable Vue components for aeternity aepp projects",
  "main": "dist/aepp-components.js",
  "scripts": {
    "test": "npm run unit && npm run lint",
    "unit": "jest",
    "build": "node build/build.js",
    "watch": "node build/watch.js",
    "dev": "webpack-dev-server --inline --progress --config build/sandbox/webpack.sandbox-dev.conf.js",
    "lint": "eslint --ext .js,.vue src",
    "prepublish": "npm test && npm run build",
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build"
  },
  "author": "Max DÃ¶rfler <max@apeunit.com> (https://github.com/muxe)",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "ethereum-blockies-png": "^0.1.2",
    "ethjs-unit": "^0.1.6",
    "normalize.css": "^7.0.0",
    "numeral": "^2.0.6",
    "string-loader": "0.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "jest": "^21.2.1",
    "node-sass": "^4.6.1",
    "sass-loader": "^6.0.6",
    "url-loader": "^0.6.2",
    "vue-jest": "^1.1.0",
    "vue-loader": "^13.5.0",
    "vue-router": "^3.0.1",
    "vue-styleguidist": "^1.3.5",
    "vue-template-compiler": "^2.5.8",
    "vue-test-utils": "^1.0.0-beta.6",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aeternity/aepp-components.git"
  },
  "bugs": {
    "url": "https://github.com/aeternity/aepp-components/issues"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "mapCoverage": true
  },
  "homepage": "https://github.com/aeternity/aepp-components#readme"
}
