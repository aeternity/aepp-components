{
  "name": "@aeternity/aepp-components",
  "version": "2.0.11",
  "description": "Reusable Vue components for aeternity aepp projects",
  "main": "dist/aepp-components.common.js",
  "module": "dist/aepp-components.esm.js",
  "browser": "dist/aepp-components.umd.js",
  "unpkg": "dist/aepp-components.umd.min.js",
  "scripts": {
    "build": "npm run build:common && npm run build:es",
    "build:common": "vue-cli-service build --target lib --name aepp-components src/index.js",
    "build:wc": "vue-cli-service build --target wc --name ae 'src/components/**/*.vue'",
    "build:wc-async": "vue-cli-service build --target wc-async --name ae 'src/components/**/*.vue'",
    "build:es": "rollup --config --environment BUILD:production",
    "lint": "vue-cli-service lint",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "vue-cli-service test:unit",
    "prepublish": "npm test",
    "prepare": "npm run build",
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build"
  },
  "author": "Max DÃ¶rfler <max@apeunit.com> (https://github.com/muxe)",
  "license": "ISC",
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "bn.js": "^4.11.8",
    "ethereum-blockies": "^0.1.1",
    "ethjs-unit": "^0.1.6",
    "normalize.css": "^7.0.0",
    "numeral": "^2.0.6",
    "vue-clickaway": "^2.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.12",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.12",
    "@vue/cli-plugin-unit-jest": "^3.0.0-rc.12",
    "@vue/cli-service": "^3.0.0-rc.12",
    "@vue/eslint-config-standard": "^3.0.0-rc.12",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "babel-plugin-annotate-pure-calls": "^0.3.0",
    "node-sass": "^4.9.0",
    "raw-loader": "^0.5.1",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "4.0.0-beta.7",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-url": "^1.4.0",
    "rollup-plugin-vue": "^4.3.2",
    "sass-loader": "^7.0.1",
    "vue-styleguidist": "^1.7.10",
    "vue-template-compiler": "^2.5.17",
    "vue-test-utils": "^1.0.0-beta.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aeternity/aepp-components.git"
  },
  "bugs": {
    "url": "https://github.com/aeternity/aepp-components/issues"
  },
  "homepage": "https://github.com/aeternity/aepp-components#readme",
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.vue"
  ]
}
