{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": [
          "> 1%",
          "last 2 versions",
          "not ie <= 8"
        ]
      }
    }],
    "stage-2"
  ],
  "plugins": [
    "transform-export-extensions",
    ["transform-runtime", {
      "moduleName": "babel-runtime",
      "polyfill": true,
      "helpers": false,
      "regenerator": false
    }],
    ["transform-object-rest-spread", {
      "useBuiltIns": true
    }]
  ],
  "env": {
    "test": {
      "presets": [
        "env",
        "stage-2"
      ],
      "plugins": [
        "transform-es2015-modules-commonjs",
        "dynamic-import-node"
      ]
    }
  }
}
